{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "declaration": true,
    "preserveSymlinks": true,
    "removeComments": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "rootDir": ".",
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "src/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/**/*.spec.ts",
    "src/**/*.rm"
  ]
}